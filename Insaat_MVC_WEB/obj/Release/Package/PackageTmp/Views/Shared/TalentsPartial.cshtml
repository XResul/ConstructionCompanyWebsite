@model IEnumerable<Insaat_MVC_WEB.Models.Entities.Talent> 
<section class="services-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-title">
                    <span>@Resources.Default.NelerYapiyoruz</span>
                    <h2>@Resources.Default.Servislerimiz</h2>
                </div>
            </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-4 col-sm-6">
                    <div class="service-item">
                        <i class="flaticon-038-five-stars"></i>
                        <h4>@item.TalentTitle</h4>
                        <p>
                            @item.MetaDescription
                        </p>
                    </div>
                </div>
            }


        </div>
    </div>
</section>

