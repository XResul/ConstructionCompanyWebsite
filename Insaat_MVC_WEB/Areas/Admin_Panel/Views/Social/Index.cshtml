@model IEnumerable<Insaat_MVC_WEB.Models.Entities.Social>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin_Panel/Views/Shared/_Layout.cshtml";
}

<h2>Sosyal Medya Sayfası</h2>

<p>
    <a href="~/Admin_Panel/Social/Create" class="btn btn-primary"><i class="fa fa-plus"></i> Yeni</a>
</p>
<table class="table">
    <tr>
   
        <th>
            @Html.DisplayNameFor(model => model.Instagram)
        </th>
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Instagram)
        </td>


        <td>
            <a href="~/Admin_Panel/Social/Edit/@item.Id" class="btn btn-warning"><i class="fa fa-pencil"></i> </a>

            <a href="~/Admin_Panel/Social/Detail/@item.Id" class="btn btn-info"><i class="fa fa-info"></i> </a>

            <a href="~/Admin_Panel/Social/Delete/@item.Id" class="btn btn-danger"><i class="fa fa-trash"></i> </a>
        </td>
    </tr>
}

</table>
